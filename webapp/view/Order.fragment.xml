<c:FragmentDefinition xmlns:layout="sap.ui.layout"
    xmlns:f="sap.ui.layout.form"
    xmlns:m="sap.m"
    xmlns:c="sap.ui.core"
    xmlns="sap.m"
    xmlns:l="sap.ui.layout">
    <VBox id="_IDGenVBox5" class="sapUiSmallMargin">
        <Table id="tableId" width="60%" items="{orders>/warenkorb}">
            <columns>
                <Column id="_IDGenColumn1" width="50%">
                    <Text id="_IDGenText1" text="Produkt" />
                </Column>
                <Column id="_IDGenColumn2" width="50%">
                    <Text id="_IDGenText2" text="Anzahl" />
                </Column>
            </columns>
            <items>
                <m:ColumnListItem  >
                    <m:cells>
                        <m:Input id="_IDGenText3" value="{orders>name}" editable="false"></m:Input>
                        <Input id="_IDGenStepInput1" value="{orders>menge}" editable="false" ></Input>
                    </m:cells>
                </m:ColumnListItem>
            </items>
        </Table>
    </VBox>
</c:FragmentDefinition>

